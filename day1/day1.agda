module day1.day1 where

data ℕ : Set where
  zero : ℕ
  succ : ℕ → ℕ
{-# BUILTIN NATURAL ℕ #-}

dist : ℕ → ℕ → ℕ
dist zero y = y
dist (succ x) zero = succ x
dist (succ x) (succ y) =  dist x y

infixl 20 _+_
_+_ : ℕ → ℕ → ℕ
zero + y = y
succ x + y = succ (x + y)

data Bool : Set where
  tt : Bool
  ff : Bool


if_then_else : {A : Set} → Bool → A → A → A
if tt then a else b = a
if ff then a else b = b

infixl 10 _≤b_
_≤b_ : ℕ → ℕ → Bool
zero ≤b y = tt
succ x ≤b zero =  ff
succ x ≤b succ y = x ≤b y

data List (A : Set) : Set where
  [] : List A
  _::_ : A → List A → List A

infixr 10 _::b_
data BVec (A : Set) : ℕ → Set where
  []b : BVec A 0
  _::b_ : {n : ℕ} → A → BVec A n → BVec A (succ n)
  raise : {n : ℕ} → BVec A n → BVec A (succ n)


filter : {m : ℕ} {A : Set} → (A → Bool) → BVec A m → BVec A m
filter p []b = []b
filter p (x ::b xs) = if p x then x ::b (filter p xs)
                            else (raise (filter p xs))
filter p (raise xs) = raise (filter p xs)

infixl 15 _++b_
_++b_ : {m n : ℕ} {A : Set} → BVec A m → BVec A n → BVec A (m + n)
[]b ++b ys = ys
(x ::b xs) ++b ys = x ::b (xs ++b ys)
raise xs ++b ys = raise (xs ++b ys)


infixl 16 _++_
_++_ : {A : Set} → List A → List A → List A
[] ++ ys = ys
(x :: xs) ++ ys = x :: (xs ++ ys)

quicksort : {m : ℕ} → BVec ℕ m → List ℕ
quicksort []b = []
quicksort (x ::b xs) = quicksort (filter (_≤b x) xs)  ++ (x :: []) ++ quicksort (filter (x ≤b_) xs)
quicksort (raise xs) = quicksort xs

map-pair : {A B C : Set} → (A → B → C) → List A → List B → List C
map-pair f [] [] = []
map-pair f [] (x :: ys) = []
map-pair f (x :: xs) [] = []
map-pair f (x :: xs) (y :: ys) = f x y :: map-pair f xs ys

sum : List ℕ → ℕ
sum [] = zero
sum (x :: xs) =  x + sum xs

solve : {m : ℕ} → BVec ℕ m → BVec ℕ m → ℕ
solve xs ys = sum (map-pair dist (quicksort xs) (quicksort ys))


